{
  "name": "@h4us/feeltool-electron-app",
  "productName": "FeelTool",
  "version": "1.0.0-alpha.1",
  "main": "main/index.js",
  "scripts": {
    "clean": "rimraf dist renderer/.next renderer/out",
    "start": "electron .",
    "dev": "nodemon --exec ./node_modules/.bin/electron .",
    "build": "next build renderer && next export renderer",
    "pack-app": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ],
    "mac": {
      "hardenedRuntime": false,
      "extendInfo": {
        "NSCameraUsageDescription": "Please give access to your webcam"
      }
    }
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "electron": "^13.0.1",
    "electron-builder": "^22.10.5",
    "electron-rebuild": "^2.3.5",
    "next": "latest",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "socket.io-client": "^4.0.1",
    "three": "^0.129.0"
  },
  "dependencies": {
    "electron-is-dev": "^2.0.0",
    "electron-next": "^3.1.5",
    "fastify": "^3.15.0",
    "fastify-autoload": "^3.7.1",
    "fastify-cors": "^6.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-socket.io": "^2.0.0",
    "fastify-static": "^4.0.1",
    "leva": "^0.9.11",
    "uarm-sdk-javascript": "github:h4us/uarm-sdk-javascript#latest-node-serialport"
  },
  "author": "HAUS inc.",
  "license": "MIT"
}
